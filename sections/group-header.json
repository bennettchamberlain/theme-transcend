{
  "type": "header",
  "name": "Header",
  "sections": {
    "header": {
      "type": "header",
      "blocks": {
        "logo": {
          "type": "logo",
          "settings": {
            "logoImage": "{{ shop.brand.logo }}",
            "logoImageSize": 80,
            "logoName": "",
            "logoFont": "helvetica_n4",
            "logoFontSize": 15,
            "logoAlignment": "flex-start"
          }
        },
        "menu": {
          "type": "menuLinks",
          "settings": {
            "main_linklist": "main-menu",
            "navigationAlignment": "center",
            "navigationDropdownAlignment": "auto"
          }
        },
        "accounts": {
          "type": "accounts",
          "settings": {
            "accountLink": true,
            "searchBar": true,
            "accountsAlignment": "flex-end"
          }
        }
      },
      "block_order": [
        "logo",
        "menu",
        "accounts"
      ],
      "settings": {
        "headerPosition": true,
        "hasBorder": false,
        "outlinedLinks": false,
        "openOnHover": false,
        "hasIcons": false,
        "accountLinkMob": true,
        "homeOverlay": false,
        "bgFill": "solid",
        "overlayBgColor": "#FFFFFF",
        "overlayTextColor": "#000000",
        "overlayBorderColor": "#000000"
      }
    }
  },
  "order": [
    "header"
  ]
}